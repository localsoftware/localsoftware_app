{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "web",
      "image": "",
      "essential": true,
      "memory": 1024,
      "environment": [
        {
          "name": "API_SERVER_HOST",
          "value": "api"
        },
        {
          "name": "API_SERVER_PORT",
          "value": "3000"
        }
      ],
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 3333
        }
      ]
    }
  ]
}